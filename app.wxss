/*搜索*/
.searchWrap{
    width:84%;
    margin:0 auto;
    height:60rpx;
    border-radius:60rpx;
    overflow:hidden;
}
.searchWrap input,.searchBtn{
    padding-left:60rpx;
    line-height: 60rpx;
    height:100%;
    width:91%;
}
.searchWrap .i-search{
    top:15rpx;
    left:15rpx;
}
.searchBtn{
    width:16%;
    padding-left:0;
}
.searchTab>view{
    width:50%;
    height:80rpx;
    line-height: 80rpx;
    border-bottom:3rpx solid #f4f4f4;
}
.searchTab>view.active{
    border-color:#ff5c00;
    color:#ff5c00;
}
