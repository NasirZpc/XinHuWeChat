.ellips{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
}
.clearfix:after{
    content:"";
    display:block;
    height:0;
    font-size: 0;
    clear:both;
    visibility:hidden;
}
