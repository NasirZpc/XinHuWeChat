.firstClassLists{
    display: flex;
    width:100%;
    height:64rpx;
    white-space: nowrap;
}
.firstClassItem{
    height:60rpx;
    padding:0 20rpx;
    margin:0 10rpx;
    line-height: 62rpx;
    border-bottom:4rpx solid #fff;
}
.firstClassItem.active{
    border-color:#ff5c00;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.navigatorSecond{
    width:25%;
}
.navigatorSecond image{
    width:74.4%;
    height:140rpx;
}
.navigatorSecond>view{
    width:100%;
}
.secondClassLists{
    height:425rpx;
}
