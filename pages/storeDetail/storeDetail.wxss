.storeDetailHeader{
    height:200rpx;
    background-size: cover;
    background-repeat:no-repeat;
    background-position:center;
}
.storeSearch{
    width:480rpx;
    padding-left: 15rpx;
    height:60rpx;
    border-radius: 60rpx;
    line-height: 60rpx;
    background: rgba(255,255,255,0.4);
    overflow: hidden;
}
.i-class,.i-storeNav{
    z-index: 1;
    top:10rpx;
}
.i-class{
    right:85rpx;
}
.i-storeNav{
    right:15rpx;
}
.storeHeader{
    width:94.5%;
    bottom:20rpx;
}

.storeHeader>image{
    width:90rpx;
    height:90rpx;
    border-radius: 10rpx;
}
.shopname{
    line-height: 90rpx;
}
.storeCollectBtn{
    background: #ff5c00;
    width:108rpx;
    height:42rpx;
    border-radius:42rpx;
    overflow: hidden;
}
.storeCollectBtn>view.text{
    line-height: 42rpx;
}
.i-storeCollect{
    top:-2rpx;
    left:-1rpx;
}
.storeNav>view{
    width:33.33%;
    height:80rpx;
    line-height:80rpx;
}
.storeNav .active{
    color:#ff5c00;
}
.storeNav>view:after{
    content:"";
    display: block;
    position: absolute;
    width:1px;
    height:50rpx;
    background: #eee;
    top:15rpx;
    right:0;
}
.storeNav>view:last-child:after{
    display: none;
}
.couponTitWrap{
        padding:20rpx 0 10rpx;
}
.couponTit{
    width:230rpx;
    height:44rpx;
    margin:0 auto;
    z-index: 2;
}
.couponTit image{
    margin:0 auto;
    width:200rpx;
    height:44rpx;
}
.couponTitLine{
    position:absolute;
    background: #999;
    width:280rpx;
    height:1rpx;
    top:41rpx;
    left:50%;
    margin-left:-140rpx;
    z-index: 1;
}

.couponItemUse{
    background-image:url('data:image/png;base64,');
}
.couponItemUsed{
    background-image: url('data:image/png;base64,')
}
.couponLists{
    padding-bottom:0;
}
.couponItem{
    width:305rpx;
    padding:20rpx;
    height:116rpx;
    margin-right:20rpx;
    background-size: 100% auto;
    background-repeat:no-repeat;
    overflow: hidden;
    margin-bottom:20rpx;
}
.couponItem:nth-of-type(2n){
    margin-right:0;
}
.wxParse-p image{
    display: block;
    width:100%!important;
}
.wxParse-img {
    overflow:hidden;
}
.storeFooter{
    height:90rpx;
    width:100%;
    bottom:0;
    left:0;
    box-shadow: 4rpx 0rpx 10rpx #999;
    z-index: 2;
}
.storeFooter>view{
    width:50%;
    height: 100%;
}
.storeFooter>view.fl:after{
    content:'';
    display: block;
    position: absolute;
    width:1rpx;
    height:100%;
    background: #ddd;
    top:0;
    right:0;
    z-index: 1;
}
.storeFooter>view .icon{
    margin:10rpx auto 0rpx;
}
